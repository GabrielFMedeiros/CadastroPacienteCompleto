
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Cadastrar</title>
    <link rel="stylesheet" href="~/Content/Css/bootstrap.css" />
    <script type="text/javascript" src="~/Content/Script/bootstrap.min.js"></script>
    <script type="text/javascript" src="~/Content/Script/jquery-3.3.1.min.js"></script>
    <script type="text/javascript" src="~/Content/Script/jquery-3.3.1.slim.min.js"></script>
</head>
<body class="bg-dark">
    <div class="row justify-content-center" style="margin-top:25px">
        <div class="col-lg-4 col-sm-12 bg-white" style="border-radius:20px; padding:7px">
            <center>
                <a href="~/Home/Index"><img src="~/Img/LogoLogin.png" width="150" height="100" style="margin-bottom:5px" /></a>

                <form>
                    <div class="form-group justify-content-center">
                        <div class="input-group">
                            <label class="sr-only" for="inlineFormInputGroup">Nome</label>
                            <div class="input-group mb-2">
                                <div class="input-group-prepend">
                                    <div class="input-group-text"><img src="~/Content/Svg/si-glyph-person-2.svg" width="10" height="10" /></div>
                                </div>
                                <input type="text" class="form-control" id="inlineFormInputGroup" placeholder="Nome">
                            </div>
                        </div>
                        <div class="input-group">
                            <label class="sr-only" for="inlineFormInputGroup">Idade</label>
                            <div class="input-group mb-2">
                                <div class="input-group-prepend">
                                    <div class="input-group-text"><img src="~/Content/Svg/si-glyph-baby.svg" width="10" height="10" /></div>
                                </div>
                                <input type="text" class="form-control" id="inlineFormInputGroup" placeholder="Idade">
                            </div>
                        </div>
                        <div class="input-group">
                            <label class="sr-only" for="inlineFormInputGroup">Sexo</label>
                            <div class="input-group mb-2">
                                <div class="input-group-prepend">
                                    <div class="input-group-text"><img src="~/Content/Svg/si-glyph-male.svg" width="10" height="10" /></div>
                                </div>
                                <input type="text" class="form-control" id="inlineFormInputGroup" placeholder="Sexo">
                            </div>
                        </div>
                        <div class="input-group">
                            <label class="sr-only" for="inlineFormInputGroup">E-mail</label>
                            <div class="input-group mb-2">
                                <div class="input-group-prepend">
                                    <div class="input-group-text"><img src="~/Content/Svg/si-glyph-mail.svg" width="10" height="10" /></div>
                                </div>
                                <input type="text" class="form-control" id="inlineFormInputGroup" placeholder="E-mail">
                            </div>
                        </div>
                        <div class="input-group">
                            <label class="sr-only" for="inlineFormInputGroup">Telefone</label>
                            <div class="input-group mb-2">
                                <div class="input-group-prepend">
                                    <div class="input-group-text"><img src="~/Content/Svg/si-glyph-phone-number.svg" width="10" height="10" /></div>
                                </div>
                                <input type="text" class="form-control" id="inlineFormInputGroup" placeholder="Telefone">
                            </div>
                        </div>
                        <div class="input-group">
                            <label class="sr-only" for="inlineFormInputGroup">Celular</label>
                            <div class="input-group mb-2">
                                <div class="input-group-prepend">
                                    <div class="input-group-text"><img src="~/Content/Svg/si-glyph-smartphone.svg" width="10" height="10" /></div>
                                </div>
                                <input type="text" class="form-control" id="inlineFormInputGroup" placeholder="Celular">
                            </div>
                        </div>
                        <div class="input-group">
                            <label class="sr-only" for="inlineFormInputGroup">CPF</label>
                            <div class="input-group mb-2">
                                <div class="input-group-prepend">
                                    <div class="input-group-text"><img src="~/Content/Svg/si-glyph-atm-card.svg" width="10" height="10" /></div>
                                </div>
                                <input type="text" class="form-control" id="inlineFormInputGroup" placeholder="CPF">
                            </div>
                        </div>
                        <div class="input-group">
                            <label class="sr-only" for="inlineFormInputGroup">Usuario</label>
                            <div class="input-group mb-2">
                                <div class="input-group-prepend">
                                    <div class="input-group-text"><img src="~/Content/Svg/si-glyph-person.svg" width="10" height="10" /></div>
                                </div>
                                <input type="text" class="form-control" id="inlineFormInputGroup" placeholder="Usuario">
                            </div>
                        </div>
                        <div class="input-group">
                            <label class="sr-only" for="inlineFormInputGroup">Senha</label>
                            <div class="input-group mb-2">
                                <div class="input-group-prepend">
                                    <div class="input-group-text"><img src="~/Content/Svg/si-glyph-foot-sign.svg" width="10" height="10" /></div>
                                </div>
                                <input type="text" class="form-control" id="inlineFormInputGroup" placeholder="Senha">
                            </div>
                        </div>
                        <div class="form-row">
                            <div class="col-auto">
                                <button class="btn btn-dark" type="button">Logar</button>
                            </div>
                            <div class="col-auto">
                                <button class="btn btn-dark" type="reset">Cancelar</button>
                            </div>
                        </div>
                    </div>
                </form>
            </center>
        </div>
    </div>
</body>
</html>
